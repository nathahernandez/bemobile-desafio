{
  "expo": {
    "name": "bemobile-desafio",
    "slug": "bemobile-desafio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bemobile-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/bemobile-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bemobile-icon-adaptive.png",
        "backgroundColor": "#1C1C1C"
      }
    },
    "web": {
      "favicon": "./assets/befavicon.png"
    }
  }
}
